<% result.forEach(function (item) { %>
<div  class="s-hojas"
    style="margin-top: 0.5rem;color: #13161c; font-family: Arial, Helvetica, sans-serif;background: #fff;">
    <div style="text-align: center;" class="p-4">
        <table style="width: 100%;">
            <tbody>
                <tr>
                    <td style="width: 15%;">
                        <img style="height: 60px" src="https://res.cloudinary.com/stebann/image/upload/v1681418309/logo-ministerio-educacion_fvcptq.png" alt="Logo" />
                    </td>
                    <td style="width: 70%;">
                        <div style="text-align: center;">
                            <span style="color:#000;font-weight: 600;font-size: 14.4px;line-height: 1.375;">
                                <%= auth.unidadeducativa %>
                            </span><br>
                            <p style="margin-top:1px;font-weight: 600;text-align: center;font-size: 12px;">
                                CONSOLIDADO ANUAL POR ASIGNATURA
                            </p>
                        </div>
                    </td>
                    <td style="width: 15%;text-align: center;">
                        <img style="width: 60px;" src="<%=auth.logo%>" alt="Logo" />
                    </td>
                </tr>
            </tbody>
        </table>
        <hr style="color: #000;">
        <table style="width: 100%;">
            <tbody>
                <tr style="font-size: 12px">
                    <td style="width: 60%;text-align: left;">
                        <span>GRADO/CURSO: <b><%= item.curso %> / <%= paralelo
                                %></b></span> <br>
                        <span>ASIGNATURA: <b><%= item.materia %></b> </span><br>
                        <span>DOCENTE: <b><%= item.docente %></b> </span>
                    </td>
                    <td style="width: 40%;text-align: right;">
                        <span>PERIODO: <b><%= item.periodo %></b></span><br>
                        <span>FECHA: <%= item.fechaA %></span>
                    </td>
                </tr>
            </tbody>
        </table>
        <table
            style="margin-top: 5px; border-color: #000;border-style: solid;border-width: 0px 1px 1px;max-width: 100%; width: 100%; border-spacing: 0; border-collapse: separate;">
            <thead>
                <tr>
                    <th
                        style="padding: 1px 3px;border-bottom: 0px;border-right: 1px solid #000;font-size: 10.4px;font-weight: 700;  text-align: center;border-top: 1px solid #000;">
                        ASIGNATURAS
                    </th>
                    <th
                        style="padding: 1px 3px;font-size: 10.4px;font-weight: 700;  text-align: center;border-top: 1px solid #000;">
                        PRIMER QUIMESTRE
                    </th>
                    <th
                        style="padding: 1px 3px;font-size: 10.4px;font-weight: 700;  text-align: center;border-top: 1px solid #000;">
                        SEGUNDO QUIMESTRE
                    </th>
                    <th
                        style="padding: 1px 3px;font-size: 10.4px;font-weight: 700;  text-align: center;border-top: 1px solid #000;">
