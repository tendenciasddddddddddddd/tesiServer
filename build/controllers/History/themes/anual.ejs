                        <table style="width: 100%;">
                            <tbody>
                                <tr style="font-size: 10.4px;">
                                    <td style="width: 14.28%;">P1</td>
                                    <td style="width: 14.28%;">P2</td>
                                    <td style="width: 14.28%;">Prom</td>
                                    <td style="width: 14.28%;">(80%)</td>
                                    <td style="width: 14.28%;">Exa</td>
                                    <td style="width: 14.28%;">(20%)</td>
                                    <td style="width: 14.28%;">Q1</td>
                                </tr>
                            </tbody>
                        </table>
                    </th>
                    <th
                        style="min-width: 125px;border-bottom: 1px solid #000;padding: 2px 3px; font-size: 10.4px;font-weight: 700; text-align: center;">
                        <table style="width: 100%;">
                            <tbody>
                                <tr style="font-size: 10.4px;">
                                    <td style="width: 14.28%;">P1</td>
                                    <td style="width: 14.28%;">P2</td>
                                    <td style="width: 14.28%;">Prom</td>
                                    <td style="width: 14.28%;">(80%)</td>
                                    <td style="width: 14.28%;">Exa</td>
                                    <td style="width: 14.28%;">(20%)</td>
                                    <td style="width: 14.28%;">Q2</td>
                                </tr>
                            </tbody>
                        </table>
                    </th>
                    <th
                        style="min-width: 125px;border-bottom: 1px solid #000;padding: 2px 3px; font-size: 10.4px;font-weight: 700;text-transform: uppercase;    text-align: center;">
                        <table style="width: 100%;">
                            <tbody>
                                <tr style="font-size: 10.4px;">
                                    <td style="width: 50%;">Suple</td>
                                    <td style="width: 50%;">P.Final</td>
                                </tr>
                            </tbody>
                        </table>
                    </th>
                </tr>
            </thead>

            <tbody>
                <% item.data.forEach(function (subItems, index) { %>
                <tr >
                    <td
                        style="text-align: left;padding: 1px 3px;border-bottom-width: 0px;border-right: 1px solid #000;  font-size: 10.4px;">
                        <%= subItems.estudiante %></td>
                    <td
                        style="padding: 1px 3px;border-bottom-width: 0px;border-right: 1px solid #000; font-size: 10.4px;text-align: center;">
                        <table style="width: 100%;">
                            <tbody>
                                <tr style="font-size: 10.4px;">
                                    <td style="padding: 0px 1px;width: 14.28%;"><%=
                                        subItems.ppa
                                        %></td>
                                    <td style="padding: 0px 1px;width: 14.28%;"><%=
                                        subItems.ppb
                                        %></td>
                                    <td style="padding: 0px 1px;width: 14.28%;"><%=
                                        subItems.sumAB
                                        %></td>
                                    <td style="padding: 0px 1px;width: 14.28%;"><%=
                                        subItems.sumAB80
                                        %></td>
                                    <td style="padding: 0px 1px;width: 14.28%;"><%=
                                        subItems.exa1
                                        %></td>
                                    <td style="padding: 0px 1px;width: 14.28%;"><%=
                                        subItems.sumAB20
                                        %></td>
                                    <td style="padding: 0px 1px; text-align: center;width: 14.28%;">
                                        <b><%= subItems.proAB %></b>
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                    </td>
                    <td
                        style="padding: 1px 3px;border-bottom-width: 0px;border-right: 1px solid #000;font-size: 10.4px;text-align: center;">
                        <table style="width: 100%;">
                            <tbody>
                                <tr style="font-size: 10.4px;">
                                    <td style="padding: 0px 1px;width: 14.28%;"><%=
                                        subItems.ppc
                                        %></td>
                                    <td style="padding: 0px 1px;width: 14.28%;"><%=
                                        subItems.ppd
                                        %></td>
                                    <td style="padding: 0px 1px;width: 14.28%;"><%=
                                        subItems.sumCD
                                        %></td>
                                    <td style="padding: 0px 1px;width: 14.28%;"><%=
                                        subItems.sumCD80
                                        %></td>
                                    <td style="padding: 0px 1px;width: 14.28%;"><%=
                                        subItems.exa2
                                        %></td>
                                    <td style="padding: 0px 1px;width: 14.28%;"><%=
                                        subItems.sumCD20
                                        %></td>
                                    <td style="padding: 0px 1px; text-align: center;width: 14.28%;">
                                        <b><%= subItems.proCD %></b>
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                    </td>
                    <td style="padding: 1px 3px;border-bottom-width: 0px;text-align: center;font-size: 10.4px;">
                        <table style="width: 100%;">
                            <tbody>
                                <tr style="font-size: 10.4px;">
                                    <td style="border-bottom-width: 0px;padding: 0px 1px; width: 50%;">
                                        <%= subItems.suple %>
                                    </td>
                                    <td style="border-bottom-width: 0px;padding: 0px 1px;width: 50%;    ">
                                        <b><%= subItems.final %></b>
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                    </td>
                </tr>
                <% }) %>
            </tbody>
        </table>
        <div style="margin-top: 5rem;">
            <table style="    width: 100%;">
                <tbody>
                    <tr>
                        <td style="width: 5%;"></td>
                        <td style="width: 45%;">
                            <div style="text-align: center;">
                                <span>
                                    <b>______________________________</b>
                                </span> <br>
                                <p style=" font-size: .875rem;">
                                    <b><%= auth.rector %></b><br>
                                    RECTOR/A
                                </p>
                            </div>
                        </td>
                        <td style="width: 45%;">
                            <div style="text-align: center;">
                                <span>
                                    <b>_____________________________</b>
                                </span> <br>
                                <p style="font-size: .875rem;">
                                    <b><%= auth.secretario %></b><br>
                                    SECRETARIO/A
                                </p>
                            </div>
                        </td>
                        <td style="width: 5%;"></td>
                    </tr>
                </tbody>
            </table>
        </div>
    </div><br>
    <div style="page-break-before:always;"></div>
</div>
