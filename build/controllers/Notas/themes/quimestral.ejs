<div class="s-hojas"
    style="margin-top: 0.5rem;color: #13161c; font-family: Arial, Helvetica, sans-serif;background: #fff;">
    <div style="text-align: center;" class="p-4">
        <table style="width: 100%;">
            <tbody>
                <tr>
                    <td style="width: 15%;">
                        <img style="height: 60px;"
                            src="https://res.cloudinary.com/stebann/image/upload/v1681418309/logo-ministerio-educacion_fvcptq.png"
                            alt="Logo" />
                    </td>
                    <td style="width: 70%;">
                        <div style="text-align: center;">
                            <span style="color:#000;font-weight: 600;font-size: 14.4px;line-height: 1.375;">
                                <%= auth.unidadeducativa %>
                            </span><br>
                            <p style="margin-top:-1px;text-align: center;font-size: 12px;">
                                CONSOLIDADO DE CALIFICACIONES POR QUIMESTRE <br>
                                <%= ops.quimestre %>
                            </p>
                        </div>
                    </td>
                    <td style="width: 15%;text-align: center;">
                        <img style="width: 60px;" src="<%=auth.logo%>" alt="Logo" />
                    </td>
                </tr>
            </tbody>
        </table>
        <table style="width: 100%;">
            <tbody>
                <tr style="font-size: 14.4px;">
                    <td style="text-align: left;">
                        <b>CURSO</b>
                        <%= result.curso %> / <%= paralelo %>
                    </td>
                    <td>
                        <b>PERIODO</b> AÑO LECTIVO <%= result.periodo %>
                    </td>
                </tr>
            </tbody>
        </table>
        <table
            style="margin-top: 10px; border-color: #000;border-style: solid;border-width: 0px 1px 1px;max-width: 100%; width: 100%; border-spacing: 0; border-collapse: separate;border-top: 1px solid #000;">
            <thead>
                <tr>
                    <th
                        style="padding: 1px 3px;border-bottom: 0px;font-size: 9.6px;font-weight: 700;border-bottom: 1px solid #000;border-right: 1px solid #000;">
                        N°
                    </th>
                    <th
                        style="text-align: left;padding: 1px 3px;border-bottom: 0px;font-size: 9.6px;font-weight: 700;border-bottom: 1px solid #000;">
                        APELLIDOS Y NOMBRES
                    </th>
                    <% result.distributivo.forEach(function (items, index) {%>
                    <th
                        style="writing-mode: vertical-lr; transform: rotate(180deg);padding: 1px 3px;border-bottom: 0px;border-right: 1px solid #000;border-top: 1px solid #000;font-size: 9.6px;font-weight: 600;">
                        <%= items.materia?.nombre?.substring(0, 22) %>
                    </th>
                    <% }) %>
                    <th
                        style="writing-mode: vertical-lr; transform: rotate(180deg);padding: 1px 3px;border-bottom: 0px;border-right: 1px solid #000;border-top: 1px solid #000;font-size: 9.6px;font-weight: 600;">
                        PROMEDIOS
                    </th>
                </tr>
            </thead>
            <tbody>
                <% result.help.forEach(function (items, index) {%>
                <tr>
                    <td
                        style="padding: 2px 3px;border-bottom-width: 0px;  font-size: 9.6px;border-right: 1px solid #000;border-bottom: 1px solid #000;">
                        <%= index + 1 %>
                    </td>
                    <td
                        style="text-align: left;padding: 2px 3px;border-bottom-width: 0px;  font-size: 9.6px;border-bottom: 1px solid #000;">
                        <%= items.fullname %>
                    </td>
                    <% items.data.forEach(function (subItems, index) { %>
                    <td
                        style="padding: 2px 3px;border-bottom-width: 0px;border-left: 1px solid #000; font-size: 10.4px;text-align: center;border-bottom: 1px solid #000;">
                        <span>
                            <span>
                                <%= subItems %>
                            </span>
                        </span>
                    </td>
                    <% }) %>
                    <td
                        style="text-align: left;padding: 2px 3px;border-left: 1px solid #000;border-bottom-width: 0px;  font-size: 9.6px;border-bottom: 1px solid #000;">
                        <b>
                            <%= items.result %>
                        </b>
                    </td>
                </tr>
                <% }) %>
                <tr>
                    <td
                        style="padding: 2px 3px;border-bottom-width: 0px;  font-size: 9.6px;border-top: 1px solid #000;">
                    </td>
                    <td
                        style="padding: 2px 3px;border-bottom-width: 0px;  font-size: 9.6px;border-top: 1px solid #000;">
                        <b> PROMEDIO</b>
                    </td>
                    <% result?.promedios.forEach(function (subItems) { %>
                    <td
                        style="padding: 2px 3px;border-bottom-width: 0px;border-left: 1px solid #000;border-top: 1px solid #000; font-size: 10.4px;text-align: center;">
                        <b><span>
                                <%= subItems %>
                            </span></b>
                    </td>
                    <% }) %>
                    <td style="border-left: 1px solid #000;"></td>
                </tr>
            </tbody>
        </table>
    </div>
</div>