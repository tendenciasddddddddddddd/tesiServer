<style>
    .s-contenedor {
        margin-top: 0.5rem;
        color: #000;
        font-family: Arial, Helvetica, sans-serif;
        background: #fff;
    }

    .s-centro {
        text-align: center
    }

    .s-derecha {
        text-align: right
    }

    .s-izquierda {
        text-align: left
    }

    .sf-12 {
        font-size: 12px;
    }

    .sf-14 {
        font-size: 14px;
    }

    .sf-16 {
        font-size: 16px;
    }

    .sf-104 {
        font-size: 10.4px;
    }

    .sfz-600 {
        font-weight: 600;
    }

    .s-parrafo {
        text-align: justify;
        line-height: 1.5;
    }

    .s-tabla {
        width: 100%;
    }

    .s-first {
        width: 15%;
    }

    .s-last {
        width: 70%;
    }

    .s-fill {
        width: 15%;
    }

    .s-img {
        height: 60px
    }

    .s-img2 {
        width: 60px
    }

    .s-tabla-base {
        margin-top: 5px;
        border-color: #000;
        border-style: solid;
        border-width: 0px 1px 1px;
        max-width: 100%;
        width: 100%;
        border-spacing: 0;
        border-collapse: separate;
    }

    .s-base-tb2 {
        margin-top: 8px;
        border-color: #000;
        border-style: solid;
        border-width: 1px 1px 1px;
        max-width: 100%;
        width: 100%;
        border-spacing: 0;
        border-collapse: separate;
    }

    .sb-button {
        border-bottom: 1px solid #000;
    }

    .sb-top {
        border-top: 1px solid #000;
    }

    .sb-right {
        border-right: 1px solid #000;
    }

    .s-pd01 {
        padding: 0px 1px;
    }

    .s-pd13 {
        padding: 1px 3px;
    }

    .s-pd23 {
        padding: 2px 3px;
    }

    .s-w-50 {
        width: 50%;
    }

    .s-w-22 {
        width: 22%;
    }

    .s-w-120 {
        width: 120px;
    }
</style>
<% result?.forEach(function (item) { %>
<div class="s-hojas s-contenedor">
    <div class="p-4 s-centro">
        <table class="s-tabla">
            <tbody>
                <tr>
                    <td class="s-first">
                        <img class="s-img" src="https://res.cloudinary.com/stebann/image/upload/v1681418309/logo-ministerio-educacion_fvcptq.png" alt="Logo" />
                    </td>
                    <td class="s-last">
                        <div class="s-centro">
                            <span class="sfz-600 sf-16">
                                <%= auth.unidadeducativa %>
                            </span><br>
                            <p class="s-centro sf-12">
                                Dirección: <%= auth.direccion %> Teléf: <%= auth.telefono %> <br>
                                <%= auth.ubicacion %>
                            </p>
                        </div>
                    </td>
                    <td class="s-fill s-derecha">
                        <img class="s-img2" src="<%=auth.logo%>" alt="Logo" />
                    </td>
                </tr>
            </tbody>
        </table>
        <hr>
        <table class="s-tabla">
            <tbody>
                <tr>
                    <td class="s-w-50 sf-12 s-izquierda">
                        Curso : <b> <%= item.curso %> / <%= item.paralelo %> </b><br>
                        Asignatura: <b> <%= item.materia %> </b> <br>
                        Profesor(a): <b> <%= item.docente %> </b>
                    </td>
                    <td class="s-w-50 sf-12 s-derecha">
                        PERIODO: <b> <%= item.periodo %> </b><br>
                        FECHA: <%= item.fechaA %>
                    </td>
                </tr>
            </tbody>
        </table>
        <table class="s-tabla-base">
            <thead>
                <tr>
                    <th class="s-pd13 sf-104 s-centro sfz-600 sb-top sb-right"> ASIGNATURAS</th>
                    <th class="s-pd13 sf-104 s-centro sfz-600 sb-top"> 1ER TRIMESTRE</th>
                    <th class="s-pd13 sf-104 s-centro sfz-600 sb-top"> 2DO TRIMESTRE</th>
                    <th class="s-pd13 sf-104 s-centro sfz-600 sb-top"> 3ER TRIMESTRE</th>
                    <th class="s-pd13 sf-104 s-centro sfz-600 sb-top"> PROMEDIO FINAL</th>
                </tr>
                <tr>
                    <th class="s-pd23 sb-right"></th>
                    <th class="s-pd23 sb-right sb-top s-centro s-w-120">
                        <table class="s-tabla">
                            <tbody>
                                <tr class="sf-104 sfz-600 s-centro">
                                    <td class="s-w-50">Prom</td>
                                    <td class="s-w-50">Cual</td>
                                </tr>
                            </tbody>
                        </table>
                    </th>
                    <th class="s-pd23 sb-right sb-top s-centro s-w-120">
                        <table class="s-tabla">
                            <tbody>
                                <tr class="sf-104 sfz-600 s-centro">
                                    <td class="s-w-50">Prom</td>
                                    <td class="s-w-50">Cual</td>
                                </tr>
                            </tbody>
                        </table>
                    </th>
                    <th class="s-pd23 sb-right sb-top s-centro s-w-120">
                        <table class="s-tabla">
                            <tbody>
                                <tr class="sf-104 sfz-600 s-centro">
                                    <td class="s-w-50">Prom</td>
                                    <td class="s-w-50">Cual</td>
                                </tr>
                            </tbody>
                        </table>
                    </th>
                    <th class="s-pd23 sb-top s-centro">
                        <table class="s-tabla">
                            <tbody>
                                <tr class="sf-104 sfz-600 s-centro">
                                    <td style="width: 40%;">Prom</td>
                                    <td style="width: 60%;">Cualitativo</td>
                                </tr>
                            </tbody>
                        </table>
                    </th>
                </tr>
            </thead>
            <tbody>
                <% item.data.forEach(function (subItems, index) { %>
                <tr>
                    <td class="s-pd13 sb-right s-centro sb-top sf-104 s-izquierda"> <%= subItems.estudiante %></td>
                    <td class="s-pd13 sb-right s-centro sb-top">
                        <table class="s-tabla">
                            <tbody>
                                <tr class="sf-104 s-centro">
                                    <td class="s-pd01 s-w-50"> <%= subItems.proAB %></td>
                                    <td class="s-pd01 s-w-50"> <%= subItems.ll1 %></td>
                                </tr>
                            </tbody>
                        </table>
                    </td>
                    <td class="s-pd13 sb-right s-centro sb-top">
                        <table class="s-tabla">
                            <tbody>
                                <tr class="sf-104 s-centro">
                                    <td class="s-pd01 s-w-50"> <%= subItems.proCD %></td>
                                    <td class="s-pd01 s-w-50"><%= subItems.ll2 %></td>
                                </tr>
                            </tbody>
                        </table>
                    </td>
                    <td class="s-pd13 sb-right s-centro sb-top">
                        <table class="s-tabla">
                            <tbody>
                                <tr class="sf-104 s-centro">
                                    <td class="s-pd01 s-w-50"> <%= subItems.proEF %></td>
                                    <td class="s-pd01 s-w-50"> <%= subItems.ll3 %></td>
                                </tr>
                            </tbody>
                        </table>
                    </td>
                    <td class="s-pd13 sb-top s-centro">
                        <table class="s-tabla">
                            <tbody>
                                <tr class="sf-104 s-centro">
                                    <td class="s-pd01 sfz-600" style="width: 40%;"> <%= subItems.final %> </td>
                                    <td class="s-pd01 sfz-600" style="width: 60%;"> <%= subItems.llf %></td>
                                </tr>
                            </tbody>
                        </table>
                    </td>
                </tr>
                <% }) %>
                <tr>
                    <td class="s-pd13 sb-top sf-104 sfz-600">PROMEDIO</td>
                    <td class="s-pd13 sb-top s-centro">
                        <table class="s-tabla">
                            <tbody>
                                <tr class="sf-104 s-centro">
                                    <td class="s-pd01 s-w-50 sfz-600"> <%= item.pPPA %></td>
                                    <td class="s-pd01 s-w-50 sfz-600"><%= item.lm1 %> </td>
                                </tr>
                            </tbody>
                        </table>
                    </td>
                    <td class="s-pd13 sb-top s-centro">
                        <table class="s-tabla">
                            <tbody>
                                <tr class="sf-104 s-centro">
                                    <td class="s-pd01 s-w-50 sfz-600"> <%= item.pPPB %></td>
                                    <td class="s-pd01 s-w-50 sfz-600"><%= item.lm2 %></td>
                                </tr>
                            </tbody>
                        </table>
                    </td>
                    <td class="s-pd13 sb-top s-centro">
                        <table class="s-tabla">
                            <tbody>
                                <tr class="sf-104 s-centro">
                                    <td class="s-pd01 s-w-50 sfz-600"><%= item.pPPC %></td>
                                    <td class="s-pd01 s-w-50 sfz-600"><%= item.lm3 %></td>
                                </tr>
                            </tbody>
                        </table>
                    </td>
                    <td class="s-pd13 sb-top s-centro">
                        <table class="s-tabla">
                            <tbody>
                                <tr class="sf-104 s-centro">
                                    <td class="s-pd01 sfz-600" style="width: 40%;"><%= item.prAB %></td>
                                    <td class="s-pd01 sfz-600" style="width: 60%;"> <%= item.lmf %></td>
                                </tr>
                            </tbody>
                        </table>
                    </td>
                </tr>
            </tbody>
        </table>
        <table class="s-tabla">
            <tbody>
                <tr>
                    <td class="s-w-22">
                        <table class="s-base-tb2">
                            <thead class="sf-104 s-centro">
                                <th></th>
                                <th>A</th>
                                <th>EP</th>
                                <th>I</th>
                                <th>NE</th>
                            </thead>
                            <% if (ops.q1) { %>
                            <tbody>
                                <tr class="sf-104 s-centro">
                                    <td class="sb-top sfz-600">Ʃ</td>
                                    <td class="sb-top"> <%= item.medAB[0] %></td>
                                    <td class="sb-top"><%= item.medAB[1] %></td>
                                    <td class="sb-top"><%= item.medAB[2] %></td>
                                    <td class="sb-top"><%= item.medAB[3] %></td>
                                </tr>
                                <tr class="sf-104 s-centro">
                                    <td class="sb-top"><b>%</b></td>
                                    <td class="sb-top"><%= Math.round((item.medAB[0] /item.data.length) *100) %>%</td>
                                    <td class="sb-top"><%= Math.round((item.medAB[1] /item.data.length) *100) %>%</td>
                                    <td class="sb-top"><%= Math.round((item.medAB[2] /item.data.length) *100) %>%</td>
                                    <td class="sb-top"><%= Math.round((item.medAB[3] /item.data.length) *100) %>%</td>
                                </tr>
                            </tbody>
                            <% } %>
                        </table>
                    </td>
                    <td class="s-w-22">
                        <% if (ops.q2) { %>
                        <table class="s-base-tb2">
                            <thead class="sf-104 s-centro">
                                <th></th>
                                <th>A</th>
                                <th>EP</th>
                                <th>I</th>
                                <th>NE</th>
                            </thead>
                            <tbody>
                                <tr class="sf-104 s-centro">
                                    <td class="sb-top"><b>Ʃ</b></td>
                                    <td class="sb-top"><%= item.medCD[0] %></td>
                                    <td class="sb-top"><%= item.medCD[1] %></td>
                                    <td class="sb-top"><%= item.medCD[2] %></td>
                                    <td class="sb-top"><%= item.medCD[3] %></td>
                                </tr>
                                <tr class="sf-104 s-centro">
                                    <td class="sb-top"><b>%</b> </td>
                                    <td class="sb-top"><%= Math.round((item.medCD[0] /item.data.length) *100) %>%</td>
                                    <td class="sb-top"><%= Math.round((item.medCD[1] /item.data.length) *100) %>%</td>
                                    <td class="sb-top"><%= Math.round((item.medCD[2] /item.data.length) *100) %>%</td>
                                    <td class="sb-top"><%= Math.round((item.medCD[3] /item.data.length) *100) %>%</td>
                                </tr>
                            </tbody>
                        </table>
                        <% } %>
                    </td>
                    <td class="s-w-22">
                        <% if (ops.q2) { %>
                        <table class="s-base-tb2">
                            <thead class="sf-104 s-centro">
                                <th></th>
                                <th>A</th>
                                <th>EP</th>
                                <th>I</th>
                                <th>NE</th>
                            </thead>
                            <tbody>
                                <tr class="sf-104 s-centro">
                                    <td class="sb-top"><b>Ʃ</b> </td>
                                    <td class="sb-top"><%= item.medEF[0] %></td>
                                    <td class="sb-top"><%= item.medEF[1] %></td>
                                    <td class="sb-top"><%= item.medEF[2] %></td>
                                    <td class="sb-top"><%= item.medEF[3] %></td>
                                </tr>
                                <tr class="sf-104 s-centro">
                                    <td class="sb-top"><b>%</b></td>
                                    <td class="sb-top"><%= Math.round((item.medEF[0] /item.data.length) *100) %>%</td>
                                    <td class="sb-top"><%= Math.round((item.medEF[1] /item.data.length) *100) %>%</td>
                                    <td class="sb-top"><%= Math.round((item.medEF[2] /item.data.length) *100) %>%</td>
                                    <td class="sb-top"><%= Math.round((item.medEF[3] /item.data.length) *100) %>%</td>
                                </tr>
                            </tbody>
                        </table>
                        <% } %>
                    </td>
                    <td class="s-w-22">
                        <% if (ops.q2 &&ops.q1) { %>
                        <table class="s-base-tb2">
                            <thead class="sf-104 s-centro">
                                <th></th>
                                <th>A</th>
                                <th>EP</th>
                                <th>I</th>
                                <th>NE</th>
                            </thead>
                            <tbody>
                                <tr class="sf-104 s-centro">
                                    <td class="sb-top"><b>Ʃ</b></td>
                                    <td class="sb-top"><%= item.medF[0] %></td>
                                    <td class="sb-top"><%= item.medF[1] %></td>
                                    <td class="sb-top"><%= item.medF[2] %></td>
                                    <td class="sb-top"><%= item.medF[3] %></td>
                                </tr>
                                <tr class="sf-104 s-centro">
                                    <td class="sb-top"><b>%</b> </td>
                                    <td class="sb-top"> <%= Math.round((item.medF[0] /item.data.length) *100) %>%</td>
                                    <td class="sb-top"> <%= Math.round((item.medF[1] /item.data.length) *100) %>%</td>
                                    <td class="sb-top"> <%= Math.round((item.medF[2] /item.data.length) *100) %>%</td>
                                    <td class="sb-top"> <%= Math.round((item.medF[3] /item.data.length) *100) %>%</td>
                                </tr>
                            </tbody>
                        </table>
                        <% } %>
                    </td>
                    <td class="sf-12">
                        <br>_______________________ <br>
                        FIRMA TUTOR
                    </td>
                </tr>
                <br>
            </tbody>
        </table>
    </div>
    <div style="page-break-before:always;"></div>
</div>
<% }) %>