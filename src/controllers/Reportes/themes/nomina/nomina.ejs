<style>
    .s-contenedor{
        margin-top: 0.5rem;color: #000; font-family: Arial, Helvetica, sans-serif;background: #fff;
    }
    .s-centro{
        text-align: center
    }
    .s-derecha{
        text-align: right
    }
    .s-izquierda{
        text-align: left
    }
    .sf-12{
        font-size: 12px;
    }
    .sf-14{
        font-size: 14px;
    }
    .sf-16{
        font-size: 16px;
    }
    .sf-20{
        font-size: 20px;
    }
    .sf-104{
        font-size: 10.4px;
    }
    .sfz-600{
        font-weight: 600;
    }
    .s-parrafo{
        text-align: justify ;line-height: 1.5;
    }
    .s-tabla{
        width: 100%;
    }
    .s-p-justify{
        width: 85%; margin-right: auto; margin-left: auto;
    }
    .s-first { width: 15%; }
    .s-last { width: 70%; }
    .s-fill { width: 15%; }
    .s-img{
        height: 60px
    }
    .s-img2{
        width: 60px
    }
    .s-base-tb{
        border-color: #000;border-style: solid;border-width: 0px 1px 1px;max-width: 100%; width: 100%; border-spacing: 0; border-collapse: separate;
    }
    .sb-top {
        border-top: 1px solid #000;
    }
</style>
<% result.forEach(function (item) { %>
<div class="s-hojas s-contenedor">
<div class="s-centro" style="padding: 1rem;">
    <table class="s-tabla">
        <tbody>
            <tr>
                <td class="s-first">
                    <img class="s-img" src="https://res.cloudinary.com/stebann/image/upload/v1681418309/logo-ministerio-educacion_fvcptq.png" alt="Logo" />
                </td>
                <td class="s-last">
                    <div class="s-centro">
                        <span class="sfz-600 sf-16">
                            <%= auth.unidadeducativa %>
                        </span><br>
                        <p class="s-centro sf-12">
                            Dirección:  <%= auth.direccion %>  Teléf: <%= auth.telefono %>  <br>
                            <%= auth.ubicacion %> 
                        </p>
                    </div>
                </td>
                <td class="s-fill">
                    <img class="s-img2" src="<%=auth.logo%>" alt="Logo" />
                </td>
            </tr>
        </tbody>
    </table>
    <hr>
    <table class="s-tabla">
        <tbody>
            <tr>
                <td class="sf-12 s-izquierda" style="width: 60%;">
                    <span>CURSO : <b><%= item.curso.nombre %> <%= item.paralelo %></b></span> <br>
                    <table style="width: 100%;">
                        <tbody>
                            <tr>
                                <td class="sf-12" >
                                    Asignatura : <br> Profesor(a) :
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </td>
                <td style="width: 40%;">
                    <div class="s-derecha sf-12">
                        <span>PERIODO: <b><%= item.periodo?.nombre %></b></span><br>
                        <span>FECHA: <%= item?.fechaA %></span>
                    </div>
                </td>
            </tr>
        </tbody>
    </table>
    <table class="s-tabla s-base-tb">
        <thead>
            <tr>
                <th class="sb-top" style="padding: 4px 5px;">
                    <table class="s-tabla">
                        <tbody>
                            <tr class="s-izquierda sf-104">
                                <td style="width: 10%;">
                                    Núm.
                                </td>
                                <td style="width: 15%;">
                                    COD.
                                </td>
                                <td style="width: 15%;">
                                    CEDULA
                                </td>
                                <td style="width: 60%;">
                                    APELLIDOS Y NOMBRES
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </th>
            </tr>
        </thead>
        <tbody>
            <% item?.data?.forEach(function (subItems, index) { %>
            <tr >
                <td class="s-izquierda sb-top">
                    <table class="s-tabla">
                        <tbody>
                            <tr class="sf-104" >
                                <td style="width: 10%;">
                                    <%= index + 1 %>
                                </td>
                                <td style="width: 15%;">
                                    <%= subItems.nmatricula %>
                                </td>
                                <td style="width: 15%;">
                                    <%= subItems.cedula %>
                                </td>
                                <td style="width: 60%;">
                                    <%= subItems.fullname %>
                                </td>
                            </tr>
                        </tbody>
                    </table>
                </td>
            </tr>
            <% }) %>
        </tbody>
    </table>
</div><br> <br>
<div style="page-break-before:always;"></div>
</div>
<% }) %>