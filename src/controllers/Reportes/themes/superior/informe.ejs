<style>
    .s-contenedor {
        margin-top: 0.5rem;
        color: #000;
        font-family: Arial, Helvetica, sans-serif;
        background: #fff;
    }

    .s-centro {
        text-align: center
    }

    .s-derecha {
        text-align: right
    }

    .s-izquierda {
        text-align: left
    }

    .sf-12 {
        font-size: 12px;
    }

    .sf-16 {
        font-size: 16px;
    }

    .sf-104 {
        font-size: 10.4px;
    }

    .sf-8 {
        font-size: 8px;
    }

    .sfz-600 {
        font-weight: 600;
    }

    .s-tabla {
        width: 100%;
    }

    .s-first {
        width: 15%;
    }

    .s-last {
        width: 70%;
    }

    .s-fill {
        width: 15%;
    }

    .s-img {
        height: 60px
    }

    .s-img2 {
        width: 60px
    }

    .s-tabla-base {
        margin-top: 7px;
        border-color: #000;
        border-style: solid;
        border-width: 0px 1px 1px;
        max-width: 100%;
        width: 100%;
        border-spacing: 0;
        border-collapse: separate;
    }

    .sb-button {
        border-bottom: 1px solid #000;
    }

    .sb-top {
        border-top: 1px solid #000;
    }

    .sb-right {
        border-right: 1px solid #000;
    }
    .sb-left {
        border-left: 1px solid #000;
    }

    .s-pd23 {
        padding: 2px 3px;
    }

    .s-pd13 {
        padding: 1px 3px;
    }

    .s-pading4 {
        padding: 0px 2px;
    }

    .s-pd01 {
        padding: 0px 1px;
    }

    .s-w-25 {
        width: 25%;
    }
    .s-w-20 {
        width: 20%;
    }

    .s-w-50 {
        width: 50%;
    }
    .s-w-33 {
        width: 33%;
    }
    .s-w-14-28 {
        width: 14.28%;
    }
    .s-w-16-6 {
        width: 16.6%;
