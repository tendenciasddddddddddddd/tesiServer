                                    <td class="s-w-25 s-pd01"> <%= subItems.pytf %></td>
                                    <td class="s-w-25 s-pd01"><b><%= subItems.promGen %></b></td>
                                    <td class="s-w-25 s-pd01"> <%= subItems.suple %></td>
                                    <td class="s-w-25 s-pd01"><b><%= subItems.final %></b></td>
                                </tr>
                            </tbody>
                        </table>
                    </td>
                </tr>
                <% } %>
                <% }) %>

                <!--//TODO PINTAR PROMEDIOS GENERALES -->
                <tr>
                    <td class="s-pd13 sb-top sf-104"><b>PROMEDIO</b></td>
                    <td class="s-pd13 sb-top">
                        <table class="s-tabla">
                            <tbody>
                                <tr class="sf-104 s-centro">
                                    <td class="s-w-16-6"><b><%= item.pAB%></b></td>
                                    <td class="s-w-16-6"></td>
                                    <td class="s-w-16-6"></td>
                                    <td class="s-w-16-6"></td>
                                    <td class="s-w-16-6"></td>
                                    <td class="s-w-16-6"> <b><%=item.pgeneral%></b> </td>
                                </tr>
                            </tbody>
                        </table>
                    </td>
                    <td class="s-pd13 sb-top">
                        <table class="s-tabla">
                            <tbody>
                                <tr class="sf-104 s-centro">
                                    <td class="s-w-16-6"><b><%= item.pCD%></b></td>
                                    <td class="s-w-16-6"></td>
                                    <td class="s-w-16-6"></td>
                                    <td class="s-w-16-6"></td>
                                    <td class="s-w-16-6"></td>
                                    <td class="s-w-16-6"> <b><%=item.pgeneral2%></b> </td>
                                </tr>
                            </tbody>
                        </table>
                    </td>
                    <td class="s-pd13 sb-top">
                        <table class="s-tabla">
                            <tbody>
                                <tr class="sf-104 s-centro">
                                    <td class="s-w-16-6"><b><%= item.pEF%></b></td>
                                    <td class="s-w-16-6"></td>
                                    <td class="s-w-16-6"></td>
                                    <td class="s-w-16-6"></td>
                                    <td class="s-w-16-6"></td>
                                    <td class="s-w-16-6"> <b><%=item.pgeneral33%></b></td>
                                </tr>
                            </tbody>
                        </table>
                    </td>
                    <td class="s-pd13 sb-top">
                        <table class="s-tabla">
                            <tbody>
                                <tr class="sf-104 s-centro">
                                    <td class="s-w-25"></td>
                                    <td class="s-w-25"></td>
                                    <td class="s-w-25"></td>
                                    <td class="s-w-25"><b><%= item.pgeneral3%></b></td>
                                </tr>
                            </tbody>
                        </table>
                    </td>
