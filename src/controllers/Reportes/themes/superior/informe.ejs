                        <table class="s-tabla">
                            <tbody>
                                <tr class="sf-104 sfz-600">
                                    <td class="s-w-16-6">INS</td>
                                    <td class="s-w-16-6">(90%)</td>
                                    <td class="s-w-16-6">Pry</td>
                                    <td class="s-w-16-6">Eva</td>
                                    <td class="s-w-16-6">(10%)</td>
                                    <td class="s-w-16-6"> <b>T2</b> </td>
                                </tr>
                            </tbody>
                        </table>
                    </th>
                    <th class="s-pd23 s-centro sb-button s-w-20">
