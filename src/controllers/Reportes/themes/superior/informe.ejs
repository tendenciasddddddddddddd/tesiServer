                <!--//TODO PINTAR MATERIAS CUALITATIVAS -->
                <% item.data.forEach(function (subItems) { %>
                <% if (subItems.computo!= 2) { %>
                <tr>
                    <td class="s-pd13 sb-top sf-104"> <%= subItems.materia %></td>
                    <td class="s-pd13 sb-top sf-104 s-centro sb-left"><%= subItems.letras1 %></td>
                    <td class="s-pd13 sb-top sf-104 s-centro sb-left"><%= subItems.letras2 %></td>
                    <td class="s-pd13 sb-top sf-104 s-centro sb-left"><%= subItems.letras3 %></td>
                    <td class="s-pd13 sb-top sf-104 s-centro sb-left"><b><%= subItems.letrasf %></b></td>
                </tr>
                <% } %>
