                <% item.data2.forEach(function (subItems) { %>
                <tr>
                    <td class="s-pd13 sb-top sf-104"><%= subItems.materia %></td>
                    <td class="s-pd13 sb-top sf-104 s-centro sb-left"><%= subItems.p1 %></td>
                    <td class="s-pd13 sb-top sf-104 s-centro sb-left"><%= subItems.p2 %></td>
                    <td class="s-pd13 sb-top sf-104 s-centro sb-left"><%= subItems.p2 %></td>
                    <td class="s-pd13 sb-top sf-104 s-centro sb-left"><b><%= subItems.final %> </b></td>
