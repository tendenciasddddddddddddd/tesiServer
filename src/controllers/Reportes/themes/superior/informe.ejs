                    <td class="s-pd13 sb-right sf-104 s-izquierda"> <%= subItems.materia %></td>
                    <td class="s-pd13 sb-right s-centro">
                        <table class="s-tabla">
                            <tbody>
                                <tr class="sf-104 s-centro">
                                    <td class="s-w-16-6 s-pd01"><%=subItems.sumAB%></td>
                                    <td class="s-w-16-6 s-pd01"><%=subItems.sumAB90%></td>
                                    <td class="s-w-16-6 s-pd01"><%=subItems.pry1%></td>
                                    <td class="s-w-16-6 s-pd01"><%=subItems.exa1%></td>
                                    <td class="s-w-16-6 s-pd01"><%=subItems.sumAB10%></td>
