                    <td class="s-pd13 sb-right sf-104 sb-button s-centro">
                        <%=subItems.p1%>
                    </td>
                    <td class="s-pd13 sb-right sf-104 sb-button s-centro">
                        <%=subItems.p2%>
                    </td>
                    <td class="s-pd13 sb-right sf-104 sb-button s-centro">
                        <%=subItems.p3%>
                    </td>
                    <td class="s-pd13 sf-104 sb-button s-centro sfz-600">
                        <%=subItems.letras%>
                    </td>
                </tr>
                <% }) %>
            </tbody>
        </table>
